{
  "name": "@ivan-angelkoski/counter",
  "version": "1.1.2",
  "description": "Test counter package with changesets automation publishing",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "lint": "eslint . --max-warnings 0"
  },
  "keywords": [
    "counter",
    "package",
    "changesets"
  ],
  "author": "Ivan Angjelkoski",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsdown": "^0.20.1",
    "vitest": "^4.0.18",
    "@repo/eslint-config": "workspace:*"
  }
}
